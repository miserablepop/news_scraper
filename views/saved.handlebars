<div id="modal1" class="modal">
    <div class="modal-content">
        <div id="modalHeader"></div>

        <div id="oldNotes">
            {{#each note}}
            {{/each}}
        </div>

        <div class="row">
            <form class="col s12">
                <div class="row">
                    <div class="input-field col s12">
                        <textarea id="textarea1" class="materialize-textarea"></textarea>
                        <label for="textarea1">Note:</label>
                    </div>
                </div>
            </form>
        </div>
    </div>   
    <div class="modal-footer">
        <a href="#!" class="modal-close waves-effect waves-green btn-flat" id="saveNote">Save Note</a>
    </div> 
</div>

<div class="container">
    <ul class="collapsible popout" id="savedResults">
        {{#each save}}
        {{/each}}
    </ul>
</div>

<script src="/js/saved.js"></script>